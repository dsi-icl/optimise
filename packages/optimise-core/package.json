{
    "name": "optimise-core",
    "version": "1.19.1",
    "description": "Optimise:MS Backend",
    "keywords": [
        "ms"
    ],
    "author": {
        "email": "f.guitton@imperial.ac.uk",
        "name": "Florian Guitton"
    },
    "repository": "github:dsi-icl/optimise",
    "main": "./src/optimiseServer.js",
    "scripts": {
        "lint": "eslint --ext .jsx --ext .js ./",
        "lint:fix": "eslint --ext .jsx --ext .js --fix ./",
        "test": "cross-env NODE_ENV=test jest --colors --verbose --detectOpenHandles --forceExit --runInBand",
        "build": "rimraf -rf ./build && cross-env NODE_ENV=production webpack --config webpack.config.server.js",
        "start": "rimraf -rf ./build && cross-env NODE_ENV=development webpack --config webpack.config.server.js"
    },
    "contributors": [
        "Pierre-Marie <p.danieau@imperial.ac.uk>",
        "Chon Sou <c.sou16@imperial.ac.uk>",
        "Eleni Pissaridou <e.pissaridou@imperial.ac.uk>"
    ],
    "license": "MIT",
    "dependencies": {
        "body-parser": "1.19.0",
        "connect-session-knex": "2.0.0",
        "csurf": "1.11.0",
        "csv-parse": "4.12.0",
        "express": "4.17.1",
        "express-session": "1.17.1",
        "express-zip": "3.0.0",
        "form-data": "3.0.0",
        "knex": "0.21.12",
        "moment": "2.29.1",
        "multer": "1.4.2",
        "nodeify": "1.0.1",
        "papaparse": "5.3.0",
        "passport": "0.4.1",
        "path": "0.12.7",
        "sqlite3": "5.0.0",
        "swagger-ui-express": "4.1.4",
        "uuid": "8.3.1",
        "ws": "7.3.1"
    },
    "devDependencies": {
        "@babel/core": "7.11.6",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/polyfill": "7.11.5",
        "@babel/preset-env": "7.11.5",
        "@stryker-mutator/core": "4.0.0",
        "@stryker-mutator/html-reporter": "3.1.0",
        "@stryker-mutator/javascript-mutator": "4.0.0",
        "@stryker-mutator/jest-runner": "4.0.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "26.6.2",
        "babel-loader": "8.1.0",
        "babel-plugin-require-context-hook": "1.0.0",
        "cross-env": "7.0.2",
        "eslint": "7.12.1",
        "eslint-plugin-promise": "4.2.1",
        "jest": "26.6.2",
        "native-ext-loader": "2.3.0",
        "rimraf": "3.0.2",
        "start-server-webpack-plugin": "2.2.5",
        "string-replace-loader": "2.3.0",
        "supertest": "6.0.0",
        "webpack": "4.44.2",
        "webpack-cli": "4.1.0",
        "webpack-node-externals": "2.5.2"
    },
    "optionalDependencies": {
        "ajv": "6.12.6",
        "fsevents": "2.1.3"
    },
    "jest": {
        "globalSetup": "<rootDir>/test/setup.js",
        "globalTeardown": "<rootDir>/test/teardown.js",
        "testEnvironment": "<rootDir>/test/environment.js",
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/.stryker-tmp/"
        ],
        "transform": {
            "^.+\\.(js|html|scss)$": "<rootDir>/test/preprocessor.js"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/.stryker-tmp/"
        ],
        "testMatch": [
            "**/__tests__/**/*.{js,jsx,mjs}",
            "**/+(*.)(spec|test).{js,jsx,mjs}"
        ]
    }
}
