{
    "name": "optimise-core",
    "version": "3.4.1",
    "description": "Optimise:MS Backend",
    "keywords": [
        "ms"
    ],
    "repository": "github:dsi-icl/optimise",
    "license": "MIT",
    "author": {
        "name": "Florian Guitton",
        "email": "f.guitton@imperial.ac.uk"
    },
    "contributors": [
        "Pierre-Marie <p.danieau@imperial.ac.uk>",
        "Chon Sou <c.sou16@imperial.ac.uk>",
        "Eleni Pissaridou <e.pissaridou@imperial.ac.uk>"
    ],
    "main": "./src/optimiseServer.js",
    "scripts": {
        "build": "rimraf ./build && cross-env NODE_ENV=production webpack --config webpack.config.server.js",
        "lint": "eslint .",
        "start": "rimraf ./build && cross-env NODE_ENV=development webpack --config webpack.config.server.js",
        "test": "cross-env NODE_ENV=test jest --colors --verbose --detectOpenHandles --forceExit --runInBand"
    },
    "jest": {
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/.stryker-tmp/"
        ],
        "globalSetup": "<rootDir>/test/setup.js",
        "globalTeardown": "<rootDir>/test/teardown.js",
        "testEnvironment": "<rootDir>/test/environment.js",
        "testMatch": [
            "**/__tests__/**/*.{js,jsx,mjs}",
            "**/+(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/.stryker-tmp/"
        ],
        "testSequencer": "<rootDir>/test/sequencer.js",
        "transform": {
            "^.+\\.(js|html|scss)$": "<rootDir>/test/preprocessor.js"
        }
    },
    "dependencies": {
        "bufferutil": "4.0.9",
        "connect-session-knex": "5.0.0",
        "csurf": "1.11.0",
        "csv-parse": "6.1.0",
        "express": "4.21.2",
        "express-rate-limit": "8.0.1",
        "express-session": "1.18.2",
        "express-zip": "3.0.0",
        "form-data": "4.0.4",
        "knex": "^3.1.0",
        "moment": "2.30.1",
        "multer": "2.0.2",
        "nodeify": "1.0.1",
        "papaparse": "5.5.3",
        "passport": "0.7.0",
        "path": "0.12.7",
        "sqlite3": "5.1.6",
        "swagger-ui-express": "5.0.1",
        "utf-8-validate": "6.0.5",
        "uuid": "11.1.0",
        "ws": "8.18.3"
    },
    "devDependencies": {
        "@stryker-mutator/core": "9.0.1",
        "@stryker-mutator/html-reporter": "3.1.0",
        "@stryker-mutator/javascript-mutator": "4.0.0",
        "@stryker-mutator/jest-runner": "9.0.1",
        "cross-env": "10.0.0",
        "jest": "30.0.5",
        "native-ext-loader": "2.3.0",
        "rimraf": "6.0.1",
        "run-script-webpack-plugin": "0.2.3",
        "string-replace-loader": "3.2.0",
        "supertest": "7.1.4",
        "webpack": "5.101.2",
        "webpack-cli": "6.0.1",
        "webpack-node-externals": "3.0.0"
    },
    "optionalDependencies": {
        "ajv": "8.17.1",
        "fsevents": "2.3.3"
    }
}