{
    "name": "optimise-core",
    "version": "2.0.1",
    "description": "Optimise:MS Backend",
    "keywords": [
        "ms"
    ],
    "author": {
        "email": "f.guitton@imperial.ac.uk",
        "name": "Florian Guitton"
    },
    "repository": "github:dsi-icl/optimise",
    "main": "./src/optimiseServer.js",
    "scripts": {
        "lint": "eslint --ext .jsx --ext .js ./",
        "lint:fix": "eslint --ext .jsx --ext .js --fix ./",
        "test": "cross-env NODE_ENV=test jest --colors --verbose --detectOpenHandles --forceExit --runInBand",
        "build": "rimraf -rf ./build && cross-env NODE_ENV=production webpack --config webpack.config.server.js",
        "start": "rimraf -rf ./build && cross-env NODE_ENV=development webpack --config webpack.config.server.js"
    },
    "contributors": [
        "Pierre-Marie <p.danieau@imperial.ac.uk>",
        "Chon Sou <c.sou16@imperial.ac.uk>",
        "Eleni Pissaridou <e.pissaridou@imperial.ac.uk>"
    ],
    "license": "MIT",
    "dependencies": {
        "body-parser": "1.19.1",
        "bufferutil": "4.0.6",
        "connect-session-knex": "2.1.1",
        "csurf": "1.11.0",
        "csv-parse": "5.0.4",
        "express": "4.17.2",
        "express-session": "1.17.2",
        "express-zip": "3.0.0",
        "form-data": "4.0.0",
        "knex": "0.95.15",
        "moment": "2.29.1",
        "multer": "1.4.4",
        "nodeify": "1.0.1",
        "papaparse": "5.3.1",
        "passport": "0.5.2",
        "path": "0.12.7",
        "sqlite3": "5.0.2",
        "swagger-ui-express": "4.3.0",
        "utf-8-validate": "5.0.8",
        "uuid": "8.3.2",
        "ws": "8.4.2"
    },
    "devDependencies": {
        "@babel/core": "7.16.12",
        "@babel/eslint-parser": "7.16.5",
        "@babel/eslint-plugin": "7.16.5",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/preset-env": "7.16.11",
        "@stryker-mutator/core": "5.6.0",
        "@stryker-mutator/html-reporter": "3.1.0",
        "@stryker-mutator/javascript-mutator": "4.0.0",
        "@stryker-mutator/jest-runner": "5.6.0",
        "babel-jest": "26.6.3",
        "babel-loader": "8.2.3",
        "babel-plugin-require-context-hook": "1.0.0",
        "cross-env": "7.0.3",
        "eslint": "8.7.0",
        "eslint-plugin-promise": "6.0.0",
        "jest": "26.6.3",
        "native-ext-loader": "2.3.0",
        "rimraf": "3.0.2",
        "start-server-webpack-plugin": "2.2.5",
        "string-replace-loader": "3.1.0",
        "supertest": "6.2.2",
        "webpack": "5.67.0",
        "webpack-cli": "4.9.1",
        "webpack-node-externals": "3.0.0"
    },
    "optionalDependencies": {
        "ajv": "8.9.0",
        "fsevents": "2.3.2"
    },
    "jest": {
        "globalSetup": "<rootDir>/test/setup.js",
        "globalTeardown": "<rootDir>/test/teardown.js",
        "testEnvironment": "<rootDir>/test/environment.js",
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/.stryker-tmp/"
        ],
        "transform": {
            "^.+\\.(js|html|scss)$": "<rootDir>/test/preprocessor.js"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/.stryker-tmp/"
        ],
        "testMatch": [
            "**/__tests__/**/*.{js,jsx,mjs}",
            "**/+(*.)(spec|test).{js,jsx,mjs}"
        ]
    }
}