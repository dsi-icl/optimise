{
    "name": "optimise-core",
    "version": "3.0.3",
    "description": "Optimise:MS Backend",
    "keywords": [
        "ms"
    ],
    "author": {
        "email": "f.guitton@imperial.ac.uk",
        "name": "Florian Guitton"
    },
    "repository": "github:dsi-icl/optimise",
    "main": "./src/optimiseServer.js",
    "scripts": {
        "lint": "eslint .",
        "test": "cross-env NODE_ENV=test jest --colors --verbose --detectOpenHandles --forceExit --runInBand",
        "build": "rimraf -rf ./build && cross-env NODE_ENV=production webpack --config webpack.config.server.js",
        "start": "rimraf -rf ./build && cross-env NODE_ENV=development webpack --config webpack.config.server.js --watch"
    },
    "contributors": [
        "Chon Sou <c.sou16@imperial.ac.uk>",
        "Pierre-Marie <p.danieau@imperial.ac.uk>",
        "Eleni Pissaridou <e.pissaridou@imperial.ac.uk>",
        "Joanna Thomas <joanna.thomas21@imperial.ac.uk>"
    ],
    "license": "MIT",
    "dependencies": {
        "body-parser": "1.20.2",
        "bufferutil": "4.0.8",
        "connect-session-knex": "4.0.2",
        "csurf": "1.11.0",
        "csv-parse": "5.5.6",
        "express": "4.19.2",
        "express-session": "1.18.0",
        "express-zip": "3.0.0",
        "form-data": "4.0.0",
        "knex": "3.1.0",
        "moment": "2.30.1",
        "multer": "1.4.5-lts.1",
        "nodeify": "1.0.1",
        "papaparse": "5.4.1",
        "passport": "0.7.0",
        "path": "0.12.7",
        "sqlite3": "5.1.6",
        "swagger-ui-express": "4.6.3",
        "utf-8-validate": "6.0.4",
        "uuid": "10.0.0",
        "ws": "8.18.0"
    },
    "devDependencies": {
        "@babel/core": "7.24.9",
        "@babel/eslint-parser": "7.24.8",
        "@babel/eslint-plugin": "7.24.7",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/preset-env": "7.24.8",
        "@stryker-mutator/core": "8.2.6",
        "@stryker-mutator/html-reporter": "3.1.0",
        "@stryker-mutator/javascript-mutator": "4.0.0",
        "@stryker-mutator/jest-runner": "8.2.6",
        "babel-jest": "24.9.0",
        "babel-loader": "8.3.0",
        "babel-plugin-require-context-hook": "1.0.0",
        "cross-env": "7.0.3",
        "eslint": "8.57.0",
        "eslint-plugin-promise": "6.6.0",
        "jest": "24.9.0",
        "native-ext-loader": "2.3.0",
        "rimraf": "6.0.1",
        "run-script-webpack-plugin": "0.2.0",
        "string-replace-loader": "3.1.0",
        "supertest": "7.0.0",
        "webpack": "5.93.0",
        "webpack-cli": "5.1.4",
        "webpack-node-externals": "3.0.0"
    },
    "optionalDependencies": {
        "ajv": "8.17.1",
        "fsevents": "2.3.3"
    },
    "jest": {
        "globalSetup": "<rootDir>/test/setup.js",
        "globalTeardown": "<rootDir>/test/teardown.js",
        "testEnvironment": "<rootDir>/test/environment.js",
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/.stryker-tmp/"
        ],
        "transform": {
            "^.+\\.(js|html|scss)$": "<rootDir>/test/preprocessor.js"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/.stryker-tmp/"
        ],
        "testMatch": [
            "**/__tests__/**/*.{js,jsx,mjs}",
            "**/+(*.)(spec|test).{js,jsx,mjs}"
        ]
    }
}