        "react-app-rewired": "2.2.1",
        "react-scripts": "3.4.4"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/"
