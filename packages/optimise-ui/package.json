        "draft-js": "0.11.7",
        "file-saver": "2.0.5",
        "history": "4.10.1",
        "immutable": "4.3.7",
        "interactjs": "1.10.27",
        "is-plain-object": "5.0.0",
        "query-string": "7.1.3",
        "moment": "2.30.1",
        "react": "17.0.2",
        "react-calendar-timeline": "0.26.7",
        "react-datepicker": "4.25.0",
        "react-dom": "17.0.2",
