{
    "name": "optimise-portal",
    "productName": "Optimise MS",
    "version": "1.9.31",
    "description": "Front-end for optimiseMS",
    "keywords": [
        "ms",
        "optimise"
    ],
    "bugs": {
        "url": "https://github.com/dsi-icl/optimise/issues"
    },
    "repository": "https://github.com/dsi-icl/optimise",
    "license": "MIT",
