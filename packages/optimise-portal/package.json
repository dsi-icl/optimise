    "main": "./electron/start.js",
    "scripts": {
        "api:dev": "npm run react:start",
        "api:start": "node ./launcher/root.js",
        "build": "npm run react:build && npm run electron:build",
        "electron:build": "build --win",
        "electron:start": "cross-env NODE_ENV=development electron ./electron/start.js",
        "lint": "eslint --ext .jsx --ext .js ./",
        "lint:fix": "eslint --ext .jsx --ext .js --fix ./",
        "prod": "npm run react:build && cross-env NODE_ENV=production npm run electron:start",
        "react:build": "node scripts/build.js",
        "react:head": "node scripts/start.js",
        "react:start": "cross-env BROWSER=none npm run react:head",
        "react:test": "node scripts/test.js -- --env=jsdom --colors --verbose --forceExit",
        "ship": "npm run react:build && build --win -p always",
        "start": "npm run react:start",
        "test": "npm run react:test"
    },
