{
    "name": "optimise-core",
    "version": "0.9.34",
    "description": "Backend for OptimiseMS",
    "main": "./src/optimiseServer.js",
    "scripts": {
        "lint": "eslint --ext .jsx --ext .js ./",
        "lint:fix": "eslint --ext .jsx --ext .js --fix ./",
        "migrate": "knex migrate:latest",
        "seed": "knex seed:run",
        "test": "jest --colors --verbose --detectOpenHandles --runInBand",
        "start": "node ./src/index.js"
    },
    "contributors": [
        "Pierre-Marie <p.danieau@imperial.ac.uk>",
        "Chon Sou <c.sou16@imperial.ac.uk>",
        "Florian Guitton <f.guitton@imperial.ac.uk>"
    ],
    "license": "MIT",
    "dependencies": {
        "body-parser": "1.18.3",
        "express": "4.16.3",
        "express-session": "1.15.6",
        "express-zip": "2.0.1",
        "knex": "0.15.2",
        "passport": "0.4.0",
        "path": "0.12.7",
        "sqlite3": "4.0.2",
        "swagger-ui-express": "3.0.10",
        "uuid": "3.3.2"
    },
    "devDependencies": {
        "chalk": "2.4.1",
        "codacy-coverage": "3.0.0",
        "babel-core": "6.26.3",
        "babel-eslint": "8.2.6",
        "babel-preset-env": "1.7.0",
        "eslint": "5.2.0",
        "jest": "23.4.1",
        "supertest": "3.1.0"
    },
    "optionalDependencies": {
        "ajv": "^6.5.2",
        "fsevents": "1.2.4"
    },
    "jest": {
        "globalSetup": "<rootDir>/test/setup.js",
        "globalTeardown": "<rootDir>/test/teardown.js",
        "testEnvironment": "<rootDir>/test/environment.js",
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/"
        ],
        "testMatch": [
            "**/__tests__/**/*.{js,jsx,mjs}",
            "**/+(*.)(spec|test).{js,jsx,mjs}"
        ]
    }
}
